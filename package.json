{
  "name": "docker-mcp-web-manager",
  "version": "1.0.0",
  "private": true,
  "description": "Docker MCP Web Manager - Dockerコンテナ管理のためのWebインターフェース",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/",
    "test:integration": "jest --testPathPattern=tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "clean": "rm -rf .next dist coverage test-results playwright-report",
    "prepare": "npm run playwright:install"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next-auth": "^5.0.0-beta.4",
    "zod": "^3.22.4",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@tailwindcss/forms": "^0.5.7",
    "@heroicons/react": "^2.0.18",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.8",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "@playwright/test": "^1.40.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "docker",
    "container",
    "management",
    "web-interface",
    "nextjs",
    "mcp",
    "devops"
  ],
  "author": "Docker MCP Web Manager Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/docker-mcp-web-manager.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/docker-mcp-web-manager/issues"
  },
  "homepage": "https://github.com/your-org/docker-mcp-web-manager#readme"
}